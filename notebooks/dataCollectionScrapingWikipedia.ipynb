    "def getHospitalsLocationPerCountry(country_name):\n",
    "    country_formatted = country_name.replace(' ', '_')\n",
    "    url = f\"https://en.wikipedia.org/wiki/List_of_hospitals_in_{country_formatted}\"\n",
    "    hospitalsWithLocatiion = []\n",
    "\n",
    "    try:\n",
    "        response = requests.get(url)\n",
    "        soup = BeautifulSoup(response.content, 'html.parser')\n",
    "        \n",
    "        tables = soup.find_all('table', class_='wikitable')\n",
    "        for table in tables:\n",
    "            rows = table.find_all('tr')\n",
    "            for row in rows[1:]:\n",
    "                cells = row.find_all('td')\n",
    "                if len(cells) >= 2:  \n",
    "                    hospital_name = cells[0].get_text(strip=True)\n",
    "                    location = cells[1].get_text(strip=True) if len(cells) > 1 else \"Unknown\"\n",
    "                    hospitalsWithLocatiion.append((country_name, hospital_name, location))\n",
    "                elif len(cells) == 1:  \n",
    "                    hospital_name = cells[0].get_text(strip=True)\n",
    "                    location = \"Unknown\"  \n",
    "                    hospitalsWithLocatiion.append((country_name, hospital_name, location))\n",
    "    except Exception as e:\n",
    "        print(f\"An error occurred while processing {country_name}: {e}\")\n",
    "\n",
    "    return hospitalsWithLocatiion\n",
    "\n",
    "# Aggregate hospitals for countries\n",
    "allHospitalsWithLocatiion = []\n",
    "for country in countries:\n",
    "    allHospitalsWithLocatiion.extend(getHospitalsLocationPerCountry(country))\n",
    "\n",
    "# Write results to a CSV file\n",
    "csvHospitalsWithLocatiion = \"hospitals_list_with_locations.csv\"\n",
    "with open(csvHospitalsWithLocatiion, mode='w', newline='', encoding='utf-8') as file:\n",
    "    writer = csv.writer(file)\n",
    "    writer.writerow([\"Country\", \"Hospital Name\", \"Location\"])\n",
    "    writer.writerows(allHospitalsWithLocatiion)\n",
    "\n",
    "print(f\"Scraping complete. Results saved to {csvHospitalsWithLocatiion}.\")\n",
    "\n",
    "# allHospitalsWithLocatiion"
   ]
  },
